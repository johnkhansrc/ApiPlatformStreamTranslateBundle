{
    "name": "johnkhansrc/api-platform-stream-translate-bundle",
    "description": "A lightweight extension allowing to translate in the ApiPlatform flow the values of the properties of an entity by a simple annotation.",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.3",
        "symfony/translation": "6.4.*",
        "api-platform/core": "^3.1",
        "doctrine/annotations": "^2.0",
        "doctrine/collections": "^2.0",
        "symfony/dependency-injection": "^v6.4",
        "doctrine/persistence": "^3.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Johnkhansrc\\ApiPlatformStreamTranslateBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "joHNkHaNsrc",
            "email": "jonathanmoreldev@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    }
}
